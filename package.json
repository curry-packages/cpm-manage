{
    "name": "cpm-manage",
    "version": "2.0.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Tools to manage the main repository of the Curry Package Manager",
    "category": [ "Packaging" ],
    "dependencies": {
        "cpm"     : ">= 2.0.0",
        "graphviz": ">= 2.0.0",
        "html"    : ">= 2.0.0"
    },
    "compilerCompatibility": {
        "pakcs": ">= 2.0.0",
        "kics2": ">= 2.0.0"
    },
    "exportedModules": [ "CPM.Manage" ],
    "executable": {
        "name": "cpm-manage",
        "main": "CPM.Manage"
    },
    "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/cpm-manage.git",
        "tag": "$version"
    }
}
