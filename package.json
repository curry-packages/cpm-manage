{
    "name": "cpm-manage",
    "version": "2.1.0",
    "author": "Michael Hanus <mh@informatik.uni-kiel.de>",
    "synopsis": "Tools to manage the main repository of the Curry Package Manager",
    "category": [ "Packaging" ],
    "dependencies": {
        "base"    : ">= 1.0.0, < 2.0.0",
        "cpm"     : ">= 2.2.0",
        "csv"     : ">= 1.0.0",
        "graphviz": ">= 2.0.0",
        "html2"   : ">= 0.0.1, < 1.0.0"
    },
    "compilerCompatibility": {
        "pakcs": ">= 2.0.0, < 3.0.0",
        "kics2": ">= 2.0.0, < 3.0.0"
    },
    "exportedModules": [ "CPM.Manage" ],
    "license": "BSD-3-Clause",
    "licenseFile": "LICENSE",
    "executable": {
        "name": "cpm-manage",
        "main": "CPM.Manage"
    },
    "source": {
        "git": "https://git.ps.informatik.uni-kiel.de/curry-packages/cpm-manage.git",
        "tag": "$version"
    }
}
